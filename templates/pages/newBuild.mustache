<h2 class="title">New build</h2>
<br><br>

<pre id="buildResult">

</pre>


<script>
$(function () {
  var xhr = new $.ajaxSettings.xhr()
   , $buildResult = $('#buildResult');

  xhr.open("GET", "/test");
  xhr.onreadystatechange = function () {
    if (xhr.readyState == 4 || xhr.readyState == 3) {
      $buildResult.html(xhr.responseText);
    }
  };
  xhr.send(null);
});
</script>
